{
  "version": 2,
  "projects": {
    "r-components": {
      "projectType": "library",
      "root": "libs/r-components",
      "sourceRoot": "libs/r-components/src",
      "prefix": "r-components",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/r-components/tsconfig.lib.json",
            "project": "libs/r-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/r-components/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/r-components/src/**/*.ts",
              "libs/r-components/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/r-components"
          ],
          "options": {
            "jestConfig": "libs/r-components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "r-components"
}